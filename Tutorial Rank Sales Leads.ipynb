    "#Data\n",
    "train = pd.read_csv(\"train.csv\", dtype={'PhoneNumber':str})\n",
    "test = pd.read_csv(\"test.csv\", dtype={'PhoneNumber':str})\n",
    "display(train.head(n=10))"
    "display(test.head(n=10))"
   ]
  },
